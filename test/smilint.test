#!/bin/sh

rm -rf smilint.out
mkdir smilint.out

for mib in ${TESTMIBS} ; do
    echo "$mib: checking SMIv2 MIB module with smilint."
    ../tools/smilint -l9 $mib > smilint.out/$mib 2>&1
done

if [ `cat smilint.out/* | grep -v 'longer than 32' | egrep -v '(Implicit node definition|Scalar object must not have)' | wc -l` -ne 0 ] ; then
	RC=1
	echo "see directory smilint.out for results."
else
	RC=0
fi

exit ${RC}
