#!/bin/sh

FORMAT=smiv2

rm -rf smidump-${FORMAT}-${FORMAT}.out
mkdir smidump-${FORMAT}-${FORMAT}.out

RC=0

for mib in ${TESTMIBS} ; do
    echo "comparing \`smidump -f ${FORMAT} dumps/$mib.${FORMAT}' output with dumps/$mib.${FORMAT}."
    ../tools/smidump -f ${FORMAT} dumps/$mib.${FORMAT} > smidump-${FORMAT}-${FORMAT}.out/$mib.${FORMAT}
    WC=`@DIFF@ smidump-${FORMAT}-${FORMAT}.out/$mib.${FORMAT} dumps/$mib.${FORMAT} | wc -l`
    if [ $WC -ne 0 ] ; then
	echo "*** smidump -f ${FORMAT} output differs, for details take a look at"
	echo "*** diff dumps/$mib.${FORMAT} smidump-${FORMAT}.out/$mib.${FORMAT}"
	RC=1
    fi
done

exit ${RC}
